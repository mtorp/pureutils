{
  "name": "simple-pure-utils",
  "version": "2.0.43",
  "description": "Funciones puras para manipulaci√≥n de objetos, arreglos, promesas y observables",
  "main": "dist/commonjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "dependencies": {
    "rxjs": "^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RafaelSalguero/pureutils"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "jest": "^27.2.1",
    "jest-cli": "^27.2.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "buildCommonJs": "tsc -m commonjs -outDir dist/commonjs -d false",
    "buildES6": "tsc -d -declarationDir dist/types",
    "build": "npm run buildES6 && npm run buildCommonJs",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "tsc": "tsc --noemit"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "author": "Rafael Salguero Iturrios <rafaelsalgueroiturrios@gmail.com>",
  "license": "UNLICENSED"
}
